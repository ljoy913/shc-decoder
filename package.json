{
  "name": "smart-health-card-decoder",
  "version": "1.0.0",
  "description": "API for decoding a QR encoded SMART Health Card",
  "browser": "umd/smart-health-card-decoder.umd.min.js",
  "module": "esm/index.js",
  "main": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && npm run tsc && npm run rollup && npm run minify",
    "test": "jest",
    "tsc": "tsc",
    "tsc:index.d.ts": "tsc -p tsconfig.d.ts.json",
    "rollup": "rollup -c",
    "minify": "uglifyjs ./umd/smart-health-card-decoder.umd.js -c -m --mangle-props keep_quoted -o ./umd/smart-health-card-decoder.umd.min.js",
    "clean": "rm -fr ./umd && rm -fr ./esm && rm -f index.d.ts"
  },
  "files": [
    "umd",
    "esm"
  ],
  "author": "Larry Joy",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.13",
    "@types/qrcode": "^1.4.2",
    "core-js": "^3.20.3",
    "jest": "^27.5.1",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "rollup": "^2.66.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "uglify-js": "^3.15.0"
  },
  "dependencies": {
    "@nuintun/qrcode": "^3.1.1",
    "axios": "^0.26.1",
    "browser-or-node": "^2.0.0",
    "fflate": "^0.7.3",
    "qrcode": "^1.5.0",
    "tslib": "^2.3.1"
  }
}
