{
  "name": "smart-health-card-decoder",
  "version": "1.0.0",
  "description": "API for decoding a QR encoded SMART Health Card",
  "browser": "dist/verifysmarthealthcard.browser.js",
  "module": "dist/verifysmarthealthcard.node.js",
  "scripts": {
    "prepare": "npm run rollup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "babel": "babel ./src/index.ts --out-dir lib",
    "rollup": "rollup -c",
    "minify": "uglifyjs ./dist/verifysmarthealthcard.browser.js -c -m --mangle-props keep_quoted -o ./dist/verifysmarthealthcard.browser.min.js",
    "build": "npm run build:types && npm run build:js",
    "clean": "rm -f ./dist/* && rm -f ./src/*.js && rm -f ./src/*.d.ts && rm -f ./src/*.d.ts.map && rm -f ./test/*.js && rm -f ./test/*.d.ts && rm -f ./test/*.d.ts.map",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "files": [
    "dist"
  ],
  "author": "Larry Joy",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.13",
    "@types/qrcode": "^1.4.2",
    "core-js": "^3.20.3",
    "jest": "^27.5.1",
    "rollup": "^2.66.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "uglify-js": "^3.15.0"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "fflate": "^0.7.3",
    "qrcode": "^1.5.0"
  }
}
